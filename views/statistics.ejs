<%- include('header') -%>

<div class='container'>
  <div class="row"><br><br><br><br></div>
  <div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
      <h1>Estadísticas</h1>
      <br><br>
      <p>Número de pacientes que sufrieron el mismo diagnóstico</p><br>
      <table class="table">
        <thead>
        <tr>
          <th>Diagnóstico</th>
          <th>Número de pacientes</th>
        </tr>
        </thead>
        <tbody>
          <% for (var i = 0; i < diagNum.length; i++) { %>
            <tr>
              <td> <%= diagNum[i][0]  %> </td>
              <td> <%= diagNum[i][1]  %> </td>
            </tr>
          <% } %>
        </tbody>
      </table>
    </div>
    <div class="col-md-3"></div>
  </div>
  <div class="row"></div>
